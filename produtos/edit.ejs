<%- include('../partials/header') %> 
<%- include('../partials/navbar') %> 

<div class="container">
    <div class="card">
        <div class="card-header">
            Cadastro de Produto
        </div>
        <div class="card-body">
            <form action="produto/atualizar" method="post">
                <label>Titulo</label>
                <input type="text" name="titulo" id="tituloProd"
                class="form-control" value="<%=  produto.titulo %>">
                <label>Descrição</label>
                <textarea name="descricao" id="descProd"
                class="form-control" value="<%=  produto.descricao %>" cols="30" rows="10"></textarea>
                <button class="btn btn-primary">Atualizar</button>
            </form>
        </div>

    </div>
</div>

<%- include('../partials/footer') %> 